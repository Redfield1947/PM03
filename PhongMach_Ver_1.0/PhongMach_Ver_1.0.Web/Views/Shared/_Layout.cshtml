<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <script src="~/Content/js/jquery.js"></script>
    <script src="~/Scripts/jquery-ui-1.11.4.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script type="text/javascript" src="/content/js/jquery-ui-1.9.2.custom.min.js"></script>

    <link rel="stylesheet" href="~/Content/kendo/2016.2.607/kendo.common-material.min.css">
    <link rel="stylesheet" href="~/Content/kendo/2016.2.607/kendo.material.min.css" />
    <link rel="stylesheet" href="~/Content/kendo/2016.2.607/kendo.material.mobile.min.css" />

    <script src="~/Scripts/kendo/2016.2.607/jquery.min.js"></script>
    <script src="~/Scripts/kendo/2016.2.607/angular.min.js"></script>
    <script src="~/Scripts/jszip.min.js"></script>
    <script src="~/Scripts/kendo/2016.2.607/kendo.all.min.js"></script>
    <script type="text/javascript" src="~/Scripts/BaseFunction.js"></script>
    <script src="~/Scripts/angular-ui-notification.min.js"></script>
    <link href="/Content/angular-ui-notification.min.css" rel="stylesheet" />

    @*<script src="@Url.Content("~/Content/admintemplate/")js/jquery-1.11.1.min.js"></script>*@
    <script src="@Url.Content("~/Content/admintemplate/")js/bootstrap.min.js"></script>
    <script src="@Url.Content("~/Content/admintemplate/")js/chart.min.js"></script>
    <script src="@Url.Content("~/Content/admintemplate/")js/chart-data.js"></script>
    <script src="@Url.Content("~/Content/admintemplate/")js/easypiechart.js"></script>
    <script src="@Url.Content("~/Content/admintemplate/")js/easypiechart-data.js"></script>
    <script src="@Url.Content("~/Content/admintemplate/")js/bootstrap-datepicker.js"></script>
    <link href="@Url.Content("~/Content/admintemplate/")css/bootstrap.min.css" rel="stylesheet">
    <link href="@Url.Content("~/Content/admintemplate/")css/datepicker3.css" rel="stylesheet">
    <link href="@Url.Content("~/Content/admintemplate/")css/styles.css" rel="stylesheet">
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#"><span>Creat</span>iv3</a>
                <ul class="user-menu">
                    <li class="dropdown pull-right">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-user"></span> User <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href="#"><span class="glyphicon glyphicon-user"></span> Profile</a></li>
                            <li><a href="#"><span class="glyphicon glyphicon-cog"></span> Settings</a></li>
                            <li><a href="#"><span class="glyphicon glyphicon-log-out"></span> Logout</a></li>
                        </ul>
                    </li>
                </ul>
            </div>

        </div><!-- /.container-fluid -->
    </nav>
    <div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
        <div class="row">
            <div id="sidebar-collapse" class="col-sm-3 col-lg-2 sidebar">
                <form role="search">
                    <div class="form-group">
                        <input type="text" class="form-control" placeholder="Search">
                    </div>
                </form>
                <ul class="nav menu">
                    <li><a href="@Url.Action("Index","LichKham")"><span class="glyphicon glyphicon-flag"></span>Lịch Khám</a></li>
                    <li><a href="@Url.Action("Index","Thuoc")"><span class="glyphicon glyphicon-tasks"></span>Tủ Thuốc</a></li>
                    <li><a href="@Url.Action("Index","HoaDon")"><span class="glyphicon glyphicon-briefcase"></span>Quản lý Tài vụ</a></li>
                    <li class="parent ">
                        <a href="#">
                            <span class="glyphicon glyphicon-user"></span> Bệnh nhân <span data-toggle="collapse" href="#sub-item-1" class="icon pull-right"><em class="glyphicon glyphicon-s glyphicon-plus"></em></span>
                        </a>
                        <ul class="children collapse" id="sub-item-1">
                            <li>
                                <a class="" href="@Url.Action("Index","BenhNhan")">
                                    <span class="glyphicon glyphicon-share-alt"></span> Quản lý hồ sơ bệnh nhân
                                </a>
                            </li>
                            <li>
                                <a class="" href="@Url.Action("Index","BenhAn")">
                                    <span class="glyphicon glyphicon-share-alt"></span> Quản lý bệnh án
                                </a>
                            </li>
                        </ul>
                    </li>
                    <li class="parent ">
                        <a href="#">
                            <span class="glyphicon glyphicon-book"></span> Quản lý nghiệp vụ <span data-toggle="collapse" href="#sub-item-2" class="icon pull-right"><em class="glyphicon glyphicon-s glyphicon-plus"></em></span>
                        </a>
                        <ul class="children collapse" id="sub-item-2">
                            <li>
                                <a class="" href="@Url.Action("Index","GiaTriMacDinh")">
                                    <span class="glyphicon glyphicon-share-alt"></span> Quy định phòng mạch
                                </a>
                            </li>
                            <li>
                                <a class="" href="#">
                                    <span class="glyphicon glyphicon-share-alt"></span> Quy định Phí Khám
                                </a>
                            </li>
                            <li>
                                <a class="" href="#">
                                    <span class="glyphicon glyphicon-share-alt"></span> Loại thuốc
                                </a>
                            </li>
                        </ul>
                    </li>

                    <li role="presentation" class="divider"></li>
                    <li><a href="dangnhap"><span class="glyphicon glyphicon-user"></span> Login Page</a></li>
                </ul>
            </div><!--/.sidebar-->
            </div>
               @RenderBody()
            </div>
            @*@Scripts.Render("~/bundles/jquery")
                @Scripts.Render("~/bundles/bootstrap")*@
            @RenderSection("scripts", required: false)
</body>
</html>
